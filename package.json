{
  "name": "SplashApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "ios11": "react-native run-ios --simulator=\"iPhone 11\"",
    "ios8": "react-native run-ios --simulator=\"iPhone 8\"",
    "ios8p": "react-native run-ios --simulator=\"iPhone 8 Plus\"",
    "ios11promax": "react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
    "release:android": "cd android && ./gradlew assembleRelease",
    "clean:android": "cd android && ./gradlew clean",
    "release:play": "cd android && ./gradlew bundleRelease",
    "pod:install":"cd ios && pod install",
    "test:apk": "react-native run-android --variant=release"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.14.3",
    "lottie-react-native": "^3.5.0",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-lottie-splash-screen": "^0.1.12",
    "react-native-reanimated": "^2.0.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^2.18.1" 
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/runtime": "^7.13.10",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.65.2",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
